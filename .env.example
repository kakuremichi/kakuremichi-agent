# Agent Configuration
# Copy this file to .env and update the values

# Control Server WebSocket URL
CONTROL_URL=ws://localhost:3001

# API Key for authentication (get this from Control Panel)
API_KEY=agt_your_api_key_here

# WireGuard private key (auto-generated if not provided)
# WIREGUARD_PRIVATE_KEY=

# Docker integration
DOCKER_ENABLED=false
DOCKER_SOCKET=/var/run/docker.sock
